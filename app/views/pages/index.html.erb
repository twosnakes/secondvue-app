
<div id="app">

<h2>Tasks</h2>
<ul>
  <li v-for="task in tasks" v-on:click="completeTask(task)">
  {{ task.text }}
  <h2 v-on:click="toggleTask(task)">{{ task.completed }}</h2>

  </li>
</ul>
<div>
  <input type="text" v-model="newTask">
  <button v-on:click="addTask()">Add Task</button>
</div>
 
 </div>
